// KiKit panelization for the Printalizer Densitometer Transmission LED Board
// This produces a 96x142mm sized panel containing 32 boards
{
    "layout": {
        "type": "grid",
        "rows": 4,
        "cols": 8,
        "rotation": "0deg",
        "renamenet": "Board_{n}-{orig}",
        "renameref": "{orig}"
    },
    "source": {
        "type": "auto",
        "tolerance": "5mm"
    },
    "cuts": {
        "type": "vcuts"
    },
    "framing": {
        "type": "railstb",
        "width": "5mm",
        "space": "0mm"
    },
    "tooling": {
        "type": "4hole",
        "hoffset": "5mm",
        "voffset": "2.5mm",
        "size": "2mm"
    },
    "fiducials": {
        "type": "3fid",
        "hoffset": "10mm",
        "voffset": "2.5mm",
        "coppersize": "1mm",
        "opening": "2mm"
    },
    "text": {
        "type": "simple",
        "text": "${TITLE} Panel",
        "anchor": "mt",
        "voffset": "2.5mm",
        "hjustify": "center"
    },
    "post": {
        "millradius": "1mm",
        "origin": "bl",
        "script": "tools/kikit-post.py",
        "scriptarg": "trans-led-board/trans-led-board.kicad_pcb"
    }
}
