// KiKit panelization for the Printalizer Densitometer Main Board
// This produces a 215x175mm sized panel containing 8 boards
{
    "layout": {
        "type": "grid",
        "rows": 4,
        "cols": 2,
        "hspace": "5mm",
        "vspace": "3mm",
        "rotation": "0deg",
        "alternation": "cols",
        "renamenet": "Board_{n}-{orig}",
        "renameref": "{orig}"
    },
    "source": {
        "type": "auto",
        "tolerance": "5mm"
    },
    "tabs": {
        "type": "annotation"
    },
    "cuts": {
        "type": "mousebites"
    },
    "framing": {
        "type": "railstb",
        "width": "5mm"
    },
    "tooling": {
        "type": "4hole",
        "hoffset": "5mm",
        "voffset": "2.5mm",
        "size": "2mm"
    },
    "fiducials": {
        "type": "3fid",
        "hoffset": "10mm",
        "voffset": "2.5mm",
        "coppersize": "1mm",
        "opening": "2mm"
    },
    "text": {
        "type": "simple",
        "text": "Printalyzer Densitometer Main Board",
        "anchor": "tl",
        "hoffset": "20mm",
        "voffset": "2.5mm",
        "hjustify": "left"
    },
    "post": {
        "copperfill": true,
        "millradius": "1mm"
    }
}
